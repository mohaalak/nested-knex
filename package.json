{
  "name": "nested-knex",
  "version": "0.1.2",
  "description": "Get typed nested object for knex.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohaalak/nested-knex.git"
  },
  "keywords": [
    "knex",
    "nested",
    "json",
    "left join",
    "inner join",
    "join",
    "nested object"
  ],
  "author": "Mohammad Hadi Aliabar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mohaalak/nested-knex/issues"
  },
  "homepage": "https://github.com/mohaalak/nested-knex#readme",
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2"
  },
  "files": [
    "lib/**/*"
  ],
  "peerDependencies": {
    "knex": "^2.4.0"
  },
  "dependencies": {
    "nesthydrationjs": "^1.0.5",
    "knex": "^2.4.0"
  }
}
